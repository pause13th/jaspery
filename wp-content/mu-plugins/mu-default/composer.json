{
  "name": "jaspery/mu-plugin",
  "description": "Default must use plugin",
  "type": "project",
  "license": "GPL",
  "authors": [
    {
      "name": "default",
      "email": "email@test.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "phpmailer/phpmailer": "dev-master",
    "php": ">=5.3.0",
    "composer/installers": "v1.0.12",
    "cmb2/cmb2": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Inc\\": "./inc"
    },
    "files": [
      "vendor/cmb2/init.php"
    ]
  },
  "extra": {
    "installer-paths": {
      "vendor/{$name}/": [
        "cmb2/cmb2"
      ]
    }
  }
}
